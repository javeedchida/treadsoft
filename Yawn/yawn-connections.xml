<yawn-config>
    <logroot name="yawn-log">c:/dev/temp</logroot>
    <runFolderPrefixDateFormat>yyyy-MM-dd_HH-mm-ss_</runFolderPrefixDateFormat>
    <connections>
        <connection name="my-database">
            <driverJar>ojdbc6.jar</driverJar>
            <driverClass>oracle.jdbc.OracleDriver</driverClass>
            <jdbcConnectionString>jdbc:oracle:thin:@//localhost:1571/service</jdbcConnectionString>
            <username>username</username>
            <password>password</password>
        </connection>
    </connections>
</yawn-config>